#include<iostream>
#include<vector>
#include<algorithm>

using namespace std;

typedef unsigned long long int ll;
typedef vector <int> x;
const char line = '\n';



int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll r = (100*101)/2;
    ll sum = 0;
    r = r*r;
    for(int i = 1; i <= 100 ;i++ )
    {
        sum += i*i;
    }
    cout<<r-sum;
    return 0;
}
