#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    long int x,y;
   long  int a=0,b=0,c=0;
    cin>>x;
    long int sum=0;
    while(x--)
    {
       cin>>y;
       a=(y-1)/3;
       b=(y-1)/5;
       c=(y-1)/15;
        
       sum=((3*(a*(a+1))/2) + (5*(b*(b+1))/2) - (15*(c*(c+1))/2));   
        cout<<sum<<endl;
        sum=0;
        a=0;
        b=0;
        c=0;       
    }
    return 0;
}
