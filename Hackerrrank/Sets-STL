#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <set>
#include <algorithm>
using namespace std;


int main() {
    set<int> x;
   int y,t,e;
   cin>>y;
   while(y--)
   {
       cin>>t>>e;
       if(t==1)
       x.insert(e);
       else if(t==2)
       x.erase(e);
       else if(t==3)
       {
        if(x.find(e)==x.end())
       cout<<"No"<<endl;
       else
       cout<<"Yes"<<endl;
       }
   }  
    return 0;
}



